<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>TwitterClicker</servlet-name>
		<servlet-class>com.appspot.TwitterClicker.TwitterClickerServlet</servlet-class>
	</servlet>
    <servlet>
        <servlet-name>Tweet</servlet-name>
        <servlet-class>com.appspot.TwitterClicker.TweetServlet</servlet-class>
    </servlet>
    
	<servlet-mapping>
		<servlet-name>TwitterClicker</servlet-name>
		<url-pattern>/twitterclicker</url-pattern>
	</servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tweet</servlet-name>
        <url-pattern>/cron/tweet</url-pattern>
    </servlet-mapping>
    
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
